{
  "name": "dental-city",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3032",
    "dev:stable": "next dev -p 3032",
    "dev:clean": "rm -rf .next .turbopack node_modules/.cache && next dev --turbopack -p 3032",
    "translate": "tsx scripts/generate-translations-incremental.ts",
    "translate:old": "tsx scripts/generate-translations.ts",
    "translate:locale": "tsx scripts/generate-translations-incremental.ts --locale",
    "translate:force": "tsx scripts/generate-translations-incremental.ts --force",
    "translate:dry-run": "tsx scripts/generate-translations-incremental.ts --dry-run",
    "check:translations": "node scripts/check-translations.js",
    "i18n:validate": "tsx scripts/validate-translations.ts",
    "i18n:sync": "tsx scripts/sync-translations.ts",
    "i18n:types": "tsx scripts/generate-translation-types.ts",
    "audit:a11y": "node scripts/a11y-audit.js",
    "audit:security": "npm audit --audit-level=high",
    "audit:security:fix": "npm audit fix",
    "build": "next build --turbopack",
    "build:prod": "npm run translate && next build --turbopack",
    "build:netlify": "bash scripts/netlify-prebuild.sh && next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "analyze": "ANALYZE=true npm run build",
    "size": "size-limit",
    "lighthouse": "lhci autorun",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/nextjs": "^10.26.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "configcat-node": "^11.4.1",
    "configcat-react": "^5.0.3",
    "isomorphic-dompurify": "2.12.0",
    "lucide-react": "^0.554.0",
    "motion": "^12.23.24",
    "next": "16.0.7",
    "next-intl": "^4.5.3",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-focus-lock": "^2.13.6",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@chromatic-com/storybook": "^4.1.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.0.7",
    "@playwright/test": "^1.56.1",
    "@size-limit/file": "^11.2.0",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-onboarding": "^10.1.4",
    "@storybook/addon-vitest": "^10.1.4",
    "@storybook/nextjs": "^10.1.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.10",
    "husky": "^9.1.7",
    "jsdom": "^24.1.3",
    "lint-staged": "^16.2.6",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "shadcn": "^3.5.0",
    "sharp": "^0.34.5",
    "size-limit": "^12.0.0",
    "storybook": "^10.1.4",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite": "^6.4.1",
    "vitest": "^4.0.15"
  }
}
